#include <iostream>

int main() {
    long long N;
    std::cin >> N;

    while (N % 2 == 0) {
        N /= 2;
    }

    while (N % 3 == 0) {
        N /= 3;
    }

    if (N == 1) {
        std::cout << "Yes" << std::endl;
    } else {
        std::cout << "No" << std::endl;
    }

    return 0;
}
