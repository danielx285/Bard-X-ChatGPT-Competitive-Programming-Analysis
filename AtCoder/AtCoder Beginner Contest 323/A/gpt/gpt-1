#include <iostream>
#include <string>

using namespace std;

int main() {
    string S;
    cin >> S;

    // Check if every even-positioned character (index starting from 0) is '0'
    for (int i = 1; i < 16; i += 2) {
        if (S[i] != '0') {
            cout << "No" << endl;
            return 0;
        }
    }

    cout << "Yes" << endl;
    return 0;
}
